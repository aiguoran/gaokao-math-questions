{
  "version": 2,
  "routes": [
    {
      "src": "/data.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "build": {
    "command": "echo 'Build complete'",
    "outputDirectory": "."
  }
}
